{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter: Run Web",
      "type": "shell",
      "command": "flutter",
      "args": [
        "run",
        "-d",
        "web"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^(.*)\\((\\d+):(\\d+)\\):\\s*(error|warning):\\s*(.*)$",
          "file": 1,
          "location": 2,
          "message": 5
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^Launching.*",
          "endsPattern": "^Application running on.*"
        }
      }
    }
  ]
}
